{
  "name": "cat-energy-16",
  "version": "16.0.0",
  "private": true,
  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "^9.7.1",
    "browser-sync": "^2.26.7",
    "gh-pages-deploy": "^0.5.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^6.1.1",
    "gulp-less": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "posthtml-include": "^1.3.2",
    "stylelint": "11.1.1",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "scripts": {
    "deploy": "gh-pages-deploy",
    "clean-source": "rimraf README.md src webroot package.json",
